{
  "name": "Buyo AdminFX",
  "mainClass": "com.buyo.adminfx.Main",
  "vmArgs": [
    "--module-path",
    "${env.JAVA_HOME}/javafx-sdk-21.0.3/lib",
    "--add-modules=javafx.controls,javafx.fxml"
  ],
  "classpath": [
    "target/classes",
    "${env.HOME}/.m2/repository/org/openjfx/javafx-controls/21.0.3/javafx-controls-21.0.3.jar",
    "${env.HOME}/.m2/repository/org/openjfx/javafx-fxml/21.0.3/javafx-fxml-21.0.3.jar",
    "${env.HOME}/.m2/repository/mysql/mysql-connector-java/8.0.33/mysql-connector-java-8.0.33.jar"
  ],
  "workingDirectory": "${workspaceFolder}",
  "env": {
    "JAVA_HOME": "C:\\Program Files\\Java\\jdk-21"
  }
}
